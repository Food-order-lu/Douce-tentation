{
    "version": 2,
    "name": "douce-tentation",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DB_TYPE": "supabase"
    }
}
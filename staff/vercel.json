{
    "version": 2,
    "name": "staff-portal",
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://douce-tentation.vercel.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}